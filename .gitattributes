* text=auto
# Forzar LF en c√≥digo multiplataforma
*.dart        text eol=lf
*.yaml        text eol=lf
*.yml         text eol=lf
*.sh          text eol=lf
*.kt          text eol=lf
*.kts         text eol=lf
*.gradle      text eol=lf
*.java        text eol=lf
*.xml         text eol=lf
*.properties  text eol=lf
*.md          text eol=lf
*.gitattributes text eol=lf
*.gitignore   text eol=lf
# Archivos Windows
*.bat         text eol=crlf
*.ps1         text eol=crlf
# Binarios
*.png binary
*.jpg binary
*.jpeg binary
*.gif binary
*.webp binary
*.ico binary
*.pdf binary
*.aab filter=lfs diff=lfs merge=lfs -text
*.mp4 filter=lfs diff=lfs merge=lfs -text
*.zip filter=lfs diff=lfs merge=lfs -text
*.apk filter=lfs diff=lfs merge=lfs -text
